  "displayName": "Mirzanevis - React, Typescript Snippets",
  "description": "Useful React and Typescript code snippets",
  "version": "0.1.0",
  "publisher": "Hamidrezana",
  "icon": "images/logo.png",
  "repository": {
    "type": "git",
